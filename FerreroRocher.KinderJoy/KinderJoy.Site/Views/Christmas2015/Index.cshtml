@model KinderJoy.Site.Models.Christmas2015.Christmas2015EventModel
@{
    ViewBag.Title = "[킨더조이] 크리스마스 이벤트";
    Layout = "~/Views/Shared/_Layout.cshtml";    
}
@section metas{
    <meta property="og:title" content="[킨더조이] 크리스마스 트리 만들기 이벤트" />
    <meta property="og:description" content="Merry Christmas! 맛있는 즐거움, 상상하는 즐거움, 함께하는 즐거움 크리스마스는 킨더조이의 세 가지 즐거움과 함께!" />
    <meta property="og:image" content="@ViewBag.SnsImage" />
    <meta property="og:url" content="https://www.kinderjoy.co.kr/event/2015-christmas?utm_source=facebook&utm_medium=share&utm_campaign=2015-christmas&mtk=@ViewBag.Mtk" />
}
@section styles{
    <link rel="stylesheet" type="text/css" href="~/Content/Christmas2015/christmas.css" />
}
