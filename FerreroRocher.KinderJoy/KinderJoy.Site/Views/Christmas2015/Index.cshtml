@section scripts{
    <script type="text/javascript" src="https://spi.maps.daum.net/imap/map_js_init/postcode.v2.js"></script>
    <script type="text/javascript" src="//developers.kakao.com/sdk/js/kakao.min.js"></script>

    <script type="text/javascript" src="~/Scripts/Common/devCommon.js"></script>
    <script type="text/javascript" src="~/Scripts/Common/snsCommon.js"></script>
    <script type="text/javascript" src="~/Scripts/Christmas2015/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Christmas2015/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Christmas2015/hsc-placeholder.js"></script>
    <script type="text/javascript" src="~/Scripts/Christmas2015/devViewModel.js"></script>
    <script type="text/javascript" src="~/Scripts/Christmas2015/christmas.js"></script>
    <script type="text/javascript">
        //카카오 javascript key
        Kakao.init('@System.Configuration.ConfigurationManager.AppSettings["sns.kakao.key"]');
        //페이스북
        window.fbAsyncInit = function () {
            FB.init({
                appId: '@System.Configuration.ConfigurationManager.AppSettings["sns.facebook.apikey"]', // App ID
                channelUrl: '//@HttpContext.Current.Request["HTTP_HOST"]/facebook-channel.php', // Channel File
                status: true, // check login status
                xfbml: true  // parse XFBML
            });
            //팝업방지..
            FB.getLoginStatus(function (response) { });
        };
        (function (d, debug) {
            var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement('script'); js.id = id; js.async = true;
            js.src = "//connect.facebook.net/ko_KR/all" + (debug ? "/debug" : "") + ".js";
            ref.parentNode.insertBefore(js, ref);
        }(document, /*debug*/false));

        // 닫기
        $('.js-chClose').on('click', function () {
            var popName = $(this).data('pop');
            var prefix = '.pop-';
            var $treeArea = $('.pop-tree');

            closePop(prefix + popName);

            if (popName !== 'post') {
                closePop('.dim');
            }

            // 개인정보 입력 팝업에서 닫기 버튼 을 눌렀을때
            // 트리만들기 팝업 띄우기
            if (popName === 'userInfo') {

                $treeArea.addClass('on');
            } else if (popName === 'sns') {

                location.href = location.pathname;
            }

        });

        //tab (경품 안내 및 당첨자 발표)
        $('.pop-winner__item').on('click', 'a', function (e) {
            e.preventDefault();
            $(this).parent().addClass('on').siblings().removeClass('on');
        });

        // stick 링크 클릭시 해당 섹션 위치로 이동
        $('.stick').on('click', 'a', function (e) {
            e.preventDefault();
            var data = $(this).data('val');
            var hash = this.hash;
            var top;

            if (data === 'package') {
                openPop('.pop-package');
                top = 0;
            } else {

                top = $(hash).offset().top;

                if (data === 'top') {
                    top -= 70;
                }
            }

            gotoSection(top);
        });


        // slider
        var toySlider = $('.toys-list').bxSlider({
            slideWidth: 92,
            maxSlides: 5,
            slideMargin: 0,
            pager: false,
            controls: false,
            nextSelector: '',
            prevSelector: ''
        });

        $('.arrow-next').on('click', function () {
            toySlider.goToNextSlide();
        });

        $('.arrow-prev').on('click', function () {
            toySlider.goToPrevSlide();
        });

        // 팝업 열기
        $('.js-btnPrize').on('click', function (e) {
            e.preventDefault();
            var loc = $(this).data('val');
            var $popName = $('.pop-winner');
            var top;

            if (loc === 'event1') {
                top = 800;
            } else if (loc === 'event2') {
                top = 2540;
            }

            $popName.css('top', top + 'px');

            openPop('.pop-winner');
        });


        // 플레이스 홀더
        $('.js-phone').placeholder({
            text: '-없이 입력해주세요'
        });

        $('.pop-card__item').eq(2).on('click', function () {
            $(this).find('input[type="radio"]').attr('checked', true);
            $(this).find('.js-txt').focus();
        });

        $('.js-txt').css('background', 'url(/Images/Christmas2015/popup/txt-card3.png) no-repeat 0 0');
        $('.js-txt').on({
            click: function () {
                $(this).css('background', '');
                $('#cardTxt3').attr('checked', true);
            },
            focus: function () {
                $(this).css('background', '');
                $('#cardTxt3').attr('checked', true);
            },
            blur: function () {
                if ($(this).val() === '') {
                    $(this).css('background', 'url(/Images/Christmas2015/popup/txt-card3.png) no-repeat 0 0');
                }
            }
        });

        $('.label-box3').on('click', function () {
            $('.js-txt').focus();
        });

        // 우편번호 검색 버튼 클릭시
        // 즉석당첨 일때와 트리만들기 일때 우편번호 팝업 위치를 조정해야 함
        $('.js-post').on('click', function (e) {
            //e.preventDefault();

            var name = $(this).data('name');
            var $popup = $('.pop-post');
            //alert(name);

            if (name === 'userInfo') {
                $popup.css({
                    top: '2430px'
                });
            }

        });

        var viewModel = new makeChristmasViewModel('@ViewBag.ChristmasEvent1');
        ko.applyBindings(viewModel);
    </script>
}