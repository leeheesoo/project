                            <div class="mix_slider">
                                <div class="toy_top_slider">
                                    <ul class="toy_bxslider1">
                                        <!-- 상의 8종 -->
                                        <li ng-click="setTop(2);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_1.png" alt=""></li>
                                        <li ng-click="setTop(1);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_2.png" alt=""></li>
                                        <li ng-click="setTop(7);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_3.png" alt=""></li>
                                        <li ng-click="setTop(6);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_4.png" alt=""></li>
                                        <li ng-click="setTop(4);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_5.png" alt=""></li>
                                        <li ng-click="setTop(3);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_6.png" alt=""></li>
                                        <li ng-click="setTop(8);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_7.png" alt=""></li>
                                        <li ng-click="setTop(5);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_top_8.png" alt=""></li>
                                    </ul>
                                </div>
                                <div class="toy_bottom_slider">
                                    <ul class="toy_bxslider2">
                                        <!-- 하의 8종 -->
                                        <li ng-click="setBottom(6);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_1.png" alt=""></li>
                                        <li ng-click="setBottom(4);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_2.png" alt=""></li>
                                        <li ng-click="setBottom(3);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_3.png" alt=""></li>
                                        <li ng-click="setBottom(1);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_4.png" alt=""></li>
