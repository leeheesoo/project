                                        <li ng-click="setBottom(5);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_6.png" alt=""></li>
                                        <li ng-click="setBottom(8);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_7.png" alt=""></li>
                                        <li ng-click="setBottom(7);"><img src="~/Images/NinjaBarbie2016/w/step3/original/bb_bottom_8.png" alt=""></li>
                                    </ul>
                                </div>
                                <div class="toy_acc_slider">
                                    <ul class="toy_bxslider3">
