@model KinderJoy.Site.Models.NinjaBarbie2016.NinjaBarbie2016UserViewModel
@{
    ViewBag.Title = "2016 닌자바비 이벤트";
    Layout = "~/Views/Shared/_Layout.cshtml";

    DateTime? now = ViewBag.DateTimeNow as DateTime?;
    //종료설정
    bool isClose = false;
    if (now.HasValue && now >= new DateTime(2017, 1, 2)) {
        isClose = true;
    }
    //테스트서버에서 테스트 참여 가능하도록 설정
    if (!Request.Url.Host.Equals("www.kinderjoy.co.kr")) {
        isClose = true;
    }
}
@section metas {
    <meta name="title" content="NEW 서프라이즈 출시 기념 이벤트" />
    <meta name="description" content="킨더조이 1,000개 증정!">
    <meta property="og:title" content="NEW 서프라이즈 출시 기념 이벤트" />
    <meta property="og:description" content="킨더조이 1,000개 증정!">
    @if (ViewBag.UserId != null) {
        <meta property="og:image" content="@ViewBag.OgImage">
        <meta property="og:url" content="http://@(Request.Url.Host)/event/2016-ninjabarbie/@(ViewBag.UserId)?utm_source=Kakaostory&utm_medium=sharing&utm_campaign=ninjabarbie2016">
    }
}
@section styles{
    <link rel="stylesheet" type="text/css" href="~/Content/js/jquery.bxslider.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/NinjaBarbie2016/ninjaBarbie.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/NinjaBarbie2016/popup.css" />
}
<div class="event_wrap">
    <h2>@ViewBag.Title</h2>
    <!-- 상단 안내 -->
    <div class="page_top">
        <div class="inner">
            <div class="page_tl"><img src="~/Images/NinjaBarbie2016/w/page_tl.png" alt="킨더조이 NEW SURPRISE"></div>
            <div class="page_txt"><img src="~/Images/NinjaBarbie2016/w/page_txt.png" alt=""></div>
            <div class="btn_xmas"><a href="javascript:openPop('pop_xmas');"><img src="~/Images/NinjaBarbie2016/w/btn_xmas.png" alt="킨더 크리스마스 팩 자세히 보기" /></a></div>
            <div class="page_btns">
                <a href="@Url.Action("Image", "Home")" target="_blank" class="GA" data-category="@ViewBag.Title" data-action="바비 8종 보러가기"><img src="~/Images/NinjaBarbie2016/w/btn_view_bb_active.png" alt="바비 8종 보러가기" /></a>
                <a href="@Url.Action("Image", "Home")" target="_blank" class="GA" data-category="@ViewBag.Title" data-action="닌자 8종 보러가기"><img src="~/Images/NinjaBarbie2016/w/btn_view_nj_active.png" alt="닌자 8종 보러가기" /></a>
            </div>
        </div>
    </div>
    <!-- 이벤트 설명 -->
    <div class="event_info">
        <div class="inner">
            <div class="info_tl"><img src="~/Images/NinjaBarbie2016/w/info_tl.png" alt="" /></div>
            <div class="info_txt"><img src="~/Images/NinjaBarbie2016/w/info_txt.png" alt="" /></div>
            <div class="info_btn"><a href="javascript:openPop('pop_benefit','dang');" class="GA" data-category="@ViewBag.Title" data-action="경품 안내"><img src="~/Images/NinjaBarbie2016/w/btn_view_dang.png" alt="당첨자 및 경품 안내" /></a></div>

        </div>
    </div>
    <!-- 이벤트 영역 시작 -->
    <div class="game_area_wrap" id="ninjabarbieevent" ng-app="event">
        <div class="inner">
            <div class="game_deco"></div>
            <div class="game_area">
                <div></div>
                <!-- 시작화면: 서프라이즈 선택 -->
                <div class="game_common select_char" style="display:;">
                    <div class="tl"><img src="~/Images/NinjaBarbie2016/w/step1/char_tl.png" alt="서프라이즈를 선택해주세요"></div>
                    <ul>
                        @if (isClose) {
                            <li>
                                <div class="btn"><a href="javascript:alert('이벤트가 종료되었습니다!');"><img src="~/Images/NinjaBarbie2016/w/step1/btn_char_bb_active.png" alt="바비 외출 준비"></a></div>
                            </li>
                            <li>
                                <div class="btn"><a href="javascript:alert('이벤트가 종료되었습니다!');"><img src="~/Images/NinjaBarbie2016/w/step1/btn_char_nj_active.png" alt="닌자 외출 준비"></a></div>
                            </li>
                        } else {
                            <li>
                                <div class="btn"><a href="javascript:;" ng-click="selectType('@KinderJoy.Domain.Entities.NinjaBarbie2016.NinjaBarbieSurprizeType.BARBIE');" class="GA" data-category="@ViewBag.Title" data-action="바비 외출 준비"><img src="~/Images/NinjaBarbie2016/w/step1/btn_char_bb_active.png" alt="바비 외출 준비"></a></div>
                            </li>
                            <li>
                                <div class="btn"><a href="javascript:;" ng-click="selectType('@KinderJoy.Domain.Entities.NinjaBarbie2016.NinjaBarbieSurprizeType.NINJA');" class="GA" data-category="@ViewBag.Title" data-action="닌자 전투 준비"><img src="~/Images/NinjaBarbie2016/w/step1/btn_char_nj_active.png" alt="닌자 외출 준비"></a></div>
                            </li>
                        }
                    </ul>
                </div>
                <!-- 개인정보 입력 -->
                <div class="game_common personal_info" style="display:none;">
                    @*<div class="btn_close"><a href="javascript:"><img src="images/btn_close.png" alt="전 단계로 돌아가기" /></a></div>*@
                    <div class="form_tl"><img src="~/Images/NinjaBarbie2016/w/step2/form_tl.png" alt="개인정보 입력 당첨자 발표 후 경품 발송을 위한 개인정보를 입력해주세요!" /></div>
                    <form name="saveEvent1Step1" class="private_info_frm" ng-submit="save()">
                        @Html.HiddenFor(x => x.SurprizeType, new { @ng_bind = "user.surprizeType" })
                        <div class="form_box">
                            <dl class="input_name">
                                <dt><img src="~/Images/NinjaBarbie2016/w/step2/input_name.png" alt="이름" /></dt>
                                <dd>@Html.TextBoxFor(x => x.Name, new { ng_model = "user.name", maxlength = 10, kr_input = "" })</dd>
                            </dl>
                            <dl class="input_phone">
                                <dt><img src="~/Images/NinjaBarbie2016/w/step2/input_phone.png" alt="휴대폰 번호" /></dt>
                                <dd>@Html.TextBoxFor(x => x.Mobile, new { type = "tel", ng_model = "user.mobile", maxlength = 11, @class = "ph_phone" })</dd>
                            </dl>
                            <dl class="input_address">
                                <dt><img src="~/Images/NinjaBarbie2016/w/step2/input_address.png" alt="주소" /></dt>
                                <dd class="dd_address1">@Html.TextBoxFor(x => x.ZipCode, new { ng_model = "user.zipcode", maxlength = 7, @class = "address1", @readonly = "readonly" })<img src="~/Images/NinjaBarbie2016/w/step2/btn_search.png" alt="우편번호" class="btn_search" ng_click="openPostCode()" /></dd>
                                <dd class="dd_address2">@Html.TextBoxFor(x => x.Address, new { ng_model = "user.address", maxlength = 150, @class = "addr1", @readonly = "readonly" })@Html.TextBoxFor(x => x.AddressDetail, new { ng_model = "user.addressDetail", maxlength = 200, @class = "ph_address", kr_input = "" })</dd>
                            </dl>
                            <dl class="input_phone">
                                <dt><img src="~/Images/NinjaBarbie2016/w/step2/input_age.png" alt="나이" /></dt>
                                <dd>@Html.TextBoxFor(x => x.Age, new { type = "tel", @class = "ph_age", ng_model = "user.age", maxlength = 3 })</dd>
                            </dl>
                            <div class="agree_policy">
                                <span><label for="agreeCheck"><input data-val="true" data-val-assertthat="개인정보보호를 위한 이용자 동의사항에 동의해 주세요." data-val-assertthat-constsmap="{}" data-val-assertthat-expression="Agree == true" data-val-assertthat-fieldsmap="{&quot;Agree&quot;:&quot;bool&quot;}" data-val-required="개인정보보호를 위한 이용자 동의사항에 동의해 주세요." id="agreeCheck" name="Agree" type="checkbox" value="true"><img src="~/Images/NinjaBarbie2016/w/step2/input_agree.png" class="txt_img" alt="개인정보의 수집/이용에 대한 동의" /></label></span><a href="@Url.Action("PrivacyPolicy", "NinjaBarbie2016")" target="_blank"><img src="~/Images/NinjaBarbie2016/w/step2/btn_policy.png" alt="자세히 보기 버튼" /></a>
                            </div>
                        </div>
                        <div class="btn_area">
                            <div class="btn"><button type="submit" style="cursor:pointer" class="GA" data-category="@ViewBag.Title" data-action="개인정보 입력 완료"><img src="~/Images/NinjaBarbie2016/w/step2/btn_entry.png" alt="입력 완료" /></button></div>
                        </div>
                    </form>
