                                    </ul>
                                </div>
                            </div>
                            <!-- 우측 슬라이더 덮게 -->
                            <div class="stage_cover"></div>
                        </div>
                        <div class="btns">
                            <img src="~/Images/NinjaBarbie2016/w/step3/bb_compelte_bb.png" alt="나만의 바비 완성" ng-click="goResult()" style="cursor:pointer" class="GA" data-category="@ViewBag.Title" data-action="나만의 바비 완성" />
                            <img src="~/Images/NinjaBarbie2016/w/step3/btn_replay.png" alt="다시 도전" ng-click="canvasRevert()" style="cursor:pointer" class="GA" data-category="@ViewBag.Title" data-action="나만의 바비 되돌리기">
