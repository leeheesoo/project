                            <dd>@Html.TextBoxFor(e => e.Address1, new { @class = "ph_address", @readonly = "readonly" })<br>@Html.TextBoxFor(e => e.Address2, new { @class = "ph_address2", maxlength = "200" })</dd>
                        </dl>*@
                    </div>
                </fieldset>

