        <style>
            .banner_wrap ul li {display:inline !important; }
        </style>
    <![endif]-->
    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="~/Scripts/jquery.form.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.parallax.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.cookie.js"></script>
    @*<script type="text/javascript" src="~/Scripts/ga.analytics.helper-0.1.js"></script>*@

    <script type="text/javascript" src="~/Scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script type="text/javascript" src="~/Scripts/expressive.annotations.validate.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script type="text/javascript" src="~/Scripts/knockout-3.4.0.js"></script>
    <script type="text/javascript">
        @*$.ga.init({
            analyticsId: '@System.Configuration.ConfigurationManager.AppSettings["google.analytics.account"]',
            obj: ".GA"
        });*@
        var contentUrl = "@Url.Content("~/")";
    </script>
    <script src="~/Scripts/js/func.js" type="text/javascript"></script>
    <script src="~/Scripts/js/controller.js" type="text/javascript"></script>
</head>
<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NQVHZQQ"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <!-- header -->
    <div class="header">
        <h1 class="header-title"><a href="@Url.Action("Index", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 메인으로" data-delay="100"><span class="a11y-hidden">kinderjoy</span></a></h1>
        <div class="menu">
            <ul class="clearfix">
                <li class="menu-item menu-item-intro"><a href="@Url.Action("About", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이란" data-delay="100"><span class="a11y-hidden">킨더조이란?</span></a></li>
                <li class="menu-item menu-item-cf"><a href="javascript:openMov()" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] TV/CF" data-delay="100"><span class="a11y-hidden">킨더조이 TV / CF</span></a></li>
                <li class="menu-item menu-item-event"><a href="@Url.Action("Index", "DisneyStarWars2018")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이 이벤트" data-delay="100"><span class="a11y-hidden">킨더조이 이벤트</span></a></li>
                <li class="menu-item menu-item-flavor"><a href="@Url.Action("Flavor", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 맛있는 즐거움" data-delay="100"><span class="a11y-hidden">맛있는 즐거움</span></a></li>
                <li class="menu-item menu-item-image"><a href="@Url.Action("Image", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 상상하는 즐거움" data-delay="100"><span class="a11y-hidden">상상하는 즐거움</span></a></li>
                <li class="menu-item menu-item-togather"><a href="@Url.Action("Together", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 함께하는 즐거움" data-delay="100"><span class="a11y-hidden">함께하는 즐거움</span></a></li>
                <li class="menu-item menu-item-magickinder"><a href="@Url.Action("Index", "MagicKinderAppLaunchingEvent")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 매직킨더" data-delay="100"><span class="a11y-hidden">매직킨더</span></a></li>
             </ul>
        </div>
    </div>
    <!-- // header -->
    <!-- 영상팝업 -->
    <div class="popup-mov">
        <div class="dim"></div>
        <!-- wrap -->
        <div class="popup-mov_wrap">
            <button class="event-pop_close js-eventClose" onclick="closeMov()"><span class="a11y-hidden">닫기</span></button>
            <div class="popup-mov_body">
                <div class="mov_cont"><iframe src="" width="900" height="506" wmode="opaque" frameborder="0" allowfullscreen="1"></iframe></div>
            </div>
        </div>
        <!-- // wrap -->
    </div>
    <!-- // 영상팝업 -->

    <div class="container body-content">
        @RenderBody()
        <!-- <hr />-->
    </div>

    <script type="text/javascript">
        // ==================================================
        // common setting
        // ==================================================
        function goPosition(to) {
            scrollReady = false;
            $("html, body").stop().animate(
				{ scrollTop: to },
				500,
				function () { scrollReady = true; }
			);
        }

        // ==================================================
        // func
        // ==================================================

        // 팝업 여닫기(외부에서 사용할수 있도록 함수 처리)
        function openPop(flag) {
            var name = '.popup-' + flag;
            $(name).addClass('on');
            goPosition(0);
        }

        // 동영상 팝업
        function openMov() {
        	var name = '.popup-mov';
        	$(name).addClass('on');
        	goPosition(0);
        	$('.mov_cont iframe').attr('src', 'https://www.youtube.com/embed/@ViewBag.TvCfKey?rel=0&showinfo=0&wmode=opaque&autoplay=1');
        }
        function closeMov() {
            $('.mov_cont iframe').attr('src', '');
        }

        //cf, event  alert
        $('.js-alert').on('click', 'a', function () {
            alert('준비중입니다.');
        });

        /* 팝업 닫기 */
        $('.js-eventClose').on('click', function () {
            $(this).parent().parent().removeClass('on');
        });

    </script>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)

    <!-- 구글 리마케팅 태그 코드 -->
    @if ((controller == "Home" && (action == "Index" || action == "About" || action == "Flavor" || action == "Image" || action == "Together")) || (controller == "NinjaBarbie2016" && action == "Index")) {
        <script type="text/javascript">
            /* <![CDATA[ */
            var google_conversion_id = 872488122;
            var google_custom_params = window.google_tag_params;
            var google_remarketing_only = true; 
            /* ]]> */
        </script>
        <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js">
        </script>
        <noscript>
            <div style="display:inline;">
                <img height="1" width="1" style="border-style:none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/872488122/?guid=ON&script=0" />
            </div>
        </noscript>
    }
    <!-- // 구글 리마케팅 태그 코드 -->

</body>
</html>