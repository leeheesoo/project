    <link rel="shortcut icon" type="image/x-icon" href="~/Images/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/reset.css" />
    <link rel="stylesheet" href="~/Content/site/css/style.css" />
