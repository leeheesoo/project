@{ 
    ViewBag.TvCfKey = (DateTime.Now >= new DateTime(2018, 11, 1)) ? "pdNRpM4rXV0" : "Ghnw1N3NF70";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta property="og:title" content="[킨더조이] TVCF 퀴즈 이벤트" />
    <meta property="og:url" content="http://www.kinderjoy.co.kr/Event/Word/Track/facebook/share" />
    <meta property="og:image" content="https://slocal101.cdnworks.com/FerreroRocher/KinderJoy/Word/facebookThum1.jpg" />
    <meta property="og:description" content="빈칸의 정답을 입력하고 킨더조이 받자!" />
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />
    <link rel="shortcut icon" type="image/x-icon" href="~/Images/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/style.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/css/event.css" />
    <script type="text/javascript" src="~/Content/js/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.parallax.js"></script>
    <script src="~/Scripts/js/jquery.cookie.js"></script>
    <script src="~/Scripts/ga.analytics.helper-0.1.js"></script>
    <script type="text/javascript">
        $.ga.init({
            analyticsId: '@System.Configuration.ConfigurationManager.AppSettings["google.analytics.account"]',
            obj: ".GA"
        });
        var contentUrl = "@Url.Content("~/")";
</script>
    <script src="~/Scripts/js/func.js"></script>
    <script src="~/Scripts/js/controller.js"></script>
</head>
<body>
    <!-- header -->
  <div class="header">
    <h1 class="header-title"><a href="@Url.Action("Index", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 메인으로" data-delay="100">kinderjoy</a></h1>
    <div class="menu">
      <ul class="menu-list">
        <li class="menu-item menu-item-intro"><a href="@Url.Action("About", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이란" data-delay="100">킨더조이란?</a></li>
        <li class="menu-item menu-item-cf"><a href="javascript:openMov()" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] TV/CF" data-delay="100">킨더조이 TV / CF</a></li>
        <li class="menu-item menu-item-event"><a href="@Url.Action("Index", "Word")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이 이벤트" data-delay="100">킨더조이 이벤트</a></li>
        <li class="menu-item menu-item-flavor"><a href="@Url.Action("Flavor", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 맛있는 즐거움" data-delay="100">맛있는 즐거움</a></li>
        <li class="menu-item menu-item-image"><a href="@Url.Action("Image", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 상상하는 즐거움" data-delay="100">상상하는 즐거움</a></li>
        <li class="menu-item menu-item-togather"><a href="@Url.Action("Together", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 함께하는 즐거움" data-delay="100">함께하는 즐거움</a></li>
      </ul>
    </div>
  </div>
  <!-- // header -->

	<!-- 영상팝업 -->
	<div class="popup-mov">
		 <div class="dim"></div>
		 <!-- wrap -->
		 <div class="popup-mov_wrap">
			  <button class="event-pop_close js-eventClose" onclick="closeMov()"><span class="skip">닫기</span></button>
			  <div class="popup-mov_body">
					<div class="mov_cont"><iframe src="" width="900" height="506" wmode="opaque" frameborder="0" allowfullscreen="1"></iframe></div>
			  </div>
		 </div>
		 <!-- // wrap -->
	</div>
	<!-- // 영상팝업 -->

   <div class="container body-content">
      @RenderBody()
      <!-- <hr />-->
   </div>

    <script type="text/javascript">
    	// ==================================================
    	// common setting
    	// ==================================================
    	function goPosition(to) {
    		scrollReady=false;
    		$("html, body").stop().animate(
				{ scrollTop: to },
				500,
				function() { scrollReady=true; }
			);
    	}

    	// ==================================================
    	// func
    	// ==================================================

    	// 팝업 여닫기(외부에서 사용할수 있도록 함수 처리)
    	function openPop(flag) {
    		var name='.popup-'+flag;
    		$(name).addClass('on');
    		goPosition(0);
    	}

    	// 동영상 팝업
    	function openMov() {
    		openPop('mov');
    		$('.mov_cont iframe').attr('src','https://www.youtube.com/embed/@ViewBag.TvCfKey?rel=0&showinfo=0&wmode=opaque&autoplay=1');
    	}
    	function closeMov() {
    		$('.mov_cont iframe').attr('src','');
    	}

        //cf, event  alert
        $('.js-alert').on('click', 'a', function () {
            alert('준비중입니다.');
        });

        /* 팝업 닫기 */
        $('.js-eventClose').on('click',function() {
        	$(this).parent().parent().removeClass('on');
        });

  </script>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>
