<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=640, user-scalable=yes" />
    <title>@ViewBag.Title</title>
    <meta property="og:title" content="[킨더조이] TVCF 퀴즈 이벤트" />
    <meta property="og:url" content="http://www.kinderjoy.co.kr/Event/Word/Track/facebook/share" />
    <meta property="og:image" content="https://slocal101.cdnworks.com/FerreroRocher/KinderJoy/Word/facebookThum1.jpg" />
    <meta property="og:description" content="빈칸의 정답을 입력하고 킨더조이 받자!" />
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />
   <link rel="shortcut icon" type="image/x-icon" href="~/Images/favicon.ico" />
  <link rel="stylesheet" type="text/css" href="~/Content/css/reset.css" />
  <link rel="stylesheet" type="text/css" href="~/Content/css/style.Mobile.css" />
  <link rel="stylesheet" type="text/css" href="~/Content/css/event.Mobile.css" />
	  <script type="text/javascript" src="~/Content/js/jquery-1.11.0.min.js"></script>
	<script type="text/javascript" src="~/Content/js/jquery.parallax.js"></script>

   <script type="text/javascript" src="~/Content/js/jquery.bxslider.min.js"></script>
	<link rel="stylesheet" type="text/css" href="~/Content/js/jquery.bxslider.css" />

    <script src="~/Scripts/js/jquery.cookie.js"></script>
    <script src="~/Scripts/ga.analytics.helper-0.1.js"></script>
    <script type="text/javascript">
        $.ga.init({
            analyticsId: '@System.Configuration.ConfigurationManager.AppSettings["google.analytics.account"]',
        obj: ".GA"
        });
        var contentUrl = "@Url.Content("~/")";
</script>
    <script src="~/Scripts/js/func.js"></script>
    <script src="~/Scripts/js/controller.js"></script>
</head>
<body>
<div class="nav_bar">
	<div class="logo"><a href="@Url.Action("Index", "Home")"><img src="~/Images/m/logo/logo.png" alt="kinderjoy"></a></div>
	<div class="icon_menu"><a href="javascript:openNav();"><img src="~/Images/m/logo/icon_menu.png" alt="menu"></a></div>
</div>
<div class="cont_navi" style="display:none;">
	<div class="btn_close"><a href="javascript:closeNav();"><img src="~/Images/m/items/close.png" alt="닫기"></a></div>
    <!-- header -->
  <div class="header">
		<h1 class="header-title"><a href="@Url.Action("Index", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 메인으로" data-delay="100">kinderjoy</a></h1>
    <div class="menu">
      <ul class="menu-list">
        <li class="menu-item menu-item-intro"><a href="@Url.Action("About", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이란" data-delay="100">킨더조이란?</a></li>
        <li class="menu-item menu-item-cf"><a href="@Url.Action("TvCF", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] TV/CF" data-delay="100">킨더조이 TV / CF</a></li>
        <li class="menu-item menu-item-event"><a href="@Url.Action("/Event/Word", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 킨더조이 이벤트" data-delay="100">킨더조이 이벤트</a></li>
        <li class="menu-item menu-item-flavor"><a href="@Url.Action("Flavor", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 맛있는 즐거움" data-delay="100">맛있는 즐거움</a></li>
        <li class="menu-item menu-item-image"><a href="@Url.Action("Image", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 상상하는 즐거움" data-delay="100">상상하는 즐거움</a></li>
        <li class="menu-item menu-item-togather"><a href="@Url.Action("Together", "Home")" class="GA" data-category="@ViewBag.GaTitle" data-action="[@ViewBag.GaTitle] [네비] 함께하는 즐거움" data-delay="100">함께하는 즐거움</a></li>
      </ul>
    </div>
  </div>
  <!-- // header -->
</div>

    <div class="container body-content">
        @RenderBody()
       <!-- <hr />-->
    </div>

    <script type="text/javascript">

        //cf, event  alert
        $('.js-alert').on('click', 'a', function () {
            alert('준비중입니다.');
        });

        function openNav() {
        	$('.cont_navi').height($('.mobile_wrap').height()).show();
        	$('.cont_navi .header').animate({right:0},500);
        }

        function closeNav() {
        	$('.cont_navi .header').animate({ right: -418 },500,function() {
        		$('.cont_navi').hide();
        	});
        }
  </script>

    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)
</body>
</html>