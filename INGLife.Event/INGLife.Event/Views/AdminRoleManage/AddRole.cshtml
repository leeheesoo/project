@{
    ViewBag.Title = "Settings > 역할관리 > 등록";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="">
            <div class="panel-heading">
                <h4></h4>
            </div>
            @using (Html.BeginForm(null, null, FormMethod.Post, new { name = "frm", id = "frm" }))
            {
            <!-- /.panel-body -->
            <div class="panel-body">
                <div class="table-responsive">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="" style="width:100%">
                            <tr>
                                <th class="text-center" style="vertical-align:middle">역할명</th>
                                <td>@(Html.TextBox("txtRole","", new { @class = "form-control" })) </td>
                            </tr>
                        </table>
                    </div>
                    <div class="text-right">
                        <button type="submit" class="btn btn-primary">저장</button>
                        <a href='@Url.Action("Index")' class="btn btn-primary">취소</a>
                    </div>
                </div>
            </div>
            }
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>
