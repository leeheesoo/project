<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=1080, user-scalable=no">
    <title>@ViewBag.Title</title>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', '@System.Configuration.ConfigurationManager.AppSettings["ga.code"]', 'auto');
      ga('send', 'pageview');
    </script>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryform")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryunobtrusive")
    @Scripts.Render("~/bundles/commonjs")
    @Scripts.Render("~/bundles/bootstrap")
    <script>
        $(function () {
            //구글애널리틱스
            $(document).on('click', '.ga', function (e) {
                var $this = $(this);
                var category = '@ViewBag.Title' || $this.attr('data-category');
                var action = $this.attr('data-action');
                var delay = $this.attr('data-delay');
                ga('send', 'event', category, action);
                if (delay && !$this.attr('target')) {
                    delay = parseInt(delay);
                    e.preventDefault();
                    var o = this;
                    setTimeout(function () {
                        document.location.href = o.href;
                    }, delay);
                }
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>